{
  "manifest_version": 3,
  "name": "Password Generator",
  "version": "1.0",
  "description": "Generates a random secured password",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": ["clipboardWrite"],
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}